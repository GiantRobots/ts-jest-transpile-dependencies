{
  "compilerOptions": {
    "outDir": "build",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "strict": true,
    "strictFunctionTypes": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "jsx": "react",
    "sourceMap": true,
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2016"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": []
}
